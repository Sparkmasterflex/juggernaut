%hgroup.green-bg{'data-open' => 'images'}
  %h3= "#{@title} Images"
  %menu.tabs.switch
    %li
      %a{:href => "/#/#{@klass.toLowerCase()}s/#{@id}"} content
    %li
      %a.active{:href => "/#/#{@klass.toLowerCase()}s/#{@id}/images"} images
      
.col.left
  %p.spinner Loading...
      
.col.right
  %p.spinner Loading...